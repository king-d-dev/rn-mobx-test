{
  "name": "base-react-native",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "react-native start",
    "preinstall": "node ./bin/preInstall",
    "android": "react-native run-android",
    "test:e2e": "detox test -c ios.sim.debug",
    "build:e2e": "detox build -c ios.sim.debug",
    "ci:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci:build:e2e": "detox build -c ios.sim.release",
    "android:clean": "rm -rf android/build && npm run clean && cd android && ./gradlew clean",
    "ios": "react-native run-ios --simulator \"iPhone 13 Pro\"",
    "ios:tablet": "react-native run-ios --simulator \"iPad Pro (12.9-inch) (5th generation)\"",
    "ios:build": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios:deploy": "cd ios && fastlane ios deploy",
    "ios:pods:reset": "cd ios/ && rm -rf Pods && rm -rf Podfile.lock && rm -rf BaseReactNative.xcworkspace",
    "ios:simulator:erase": "xcrun simctl erase all",
    "ios:clean": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && watchman watch-del-all && rm -rf ios/build",
    "ios:clear": "rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && npm run ios:clean && npm run ios:pods:reset && npm i",
    "compile": "tsc --noEmit -p . --pretty",
    "clear": "npm run android:clean && npm run ios:clean && rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && npm i",
    "clean": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && watchman watch-del-all",
    "format": "npm run format:md && npm run format:ts",
    "format:js": "prettier --write \"app/**/*.js\"",
    "format:json": "prettier --write \"app/**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"app/**/*.ts{,x}\"",
    "lint": "eslint index.js app test --fix --ext .ts,.tsx && npm run format",
    "patch": "patch-package",
    "postinstall": "node ./bin/postInstall",
    "prepare": "",
    "testDebug": "jest -o --watch --coverage=false",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/analytics": "~15.6.0",
    "@react-native-firebase/app": "~15.6.0",
    "@react-native-firebase/crashlytics": "~15.6.0",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.3.0",
    "@sentry/react-native": "4.2.2",
    "apisauce": "^2.1.5",
    "expo": "^46.0.0",
    "expo-application": "~4.0.1",
    "expo-asset": "~8.4.6",
    "expo-constants": "~13.2.4",
    "expo-font": "~10.2.0",
    "expo-modules-core": "~0.11.5",
    "expo-network": "~4.3.0",
    "expo-secure-store": "~11.3.0",
    "mobx": "6.6.0",
    "mobx-react-lite": "3.4.0",
    "mobx-state-tree": "5.1.5",
    "ramda": "^0.27.1",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-keychain": "^8.0.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-reanimated": "~2.9.1",
    "react-native-render-html": "^6.3.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-share": "^7.3.7",
    "react-native-webview": "11.23.0",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-decorators": "7.18.2",
    "@babel/plugin-proposal-optional-catch-binding": "7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.18.3",
    "@react-native-community/cli-platform-ios": "^8.0.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/faker": "^5.5.9",
    "@types/jasmine": "^3.8.2",
    "@types/jest": "26.0.19",
    "@types/ramda": "^0.27.58",
    "@types/react": "^17",
    "@types/react-native": "0.67.8",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.5",
    "detox": "19.7.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "4.0.0",
    "expo-modules-autolinking": "0.10.1",
    "fbjs-scripts": "3.0.1",
    "jest": "26",
    "jest-circus": "26",
    "jest-environment-node": "26",
    "jest-expo": "^44.0.1",
    "jetifier": "2.0.0",
    "metro-react-native-babel-preset": "0.71.1",
    "mocha": "6",
    "patch-package": "6.4.7",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.6.2",
    "react-devtools-core": "4.24.7",
    "react-dom": "18.0.0",
    "react-test-renderer": "18.0.0",
    "typescript": "4.7.3"
  },
  "resolutions": {
    "@expo/config-plugins": "~5.0.0",
    "@expo/prebuild-config": "~5.0.0"
  },
  "detox": {
    "test-runner": "jest",
    "runnerConfig": "e2e/config.json",
    "skipLegacyWorkersInjection": true,
    "devices": {
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 13 Pro Max"
        }
      }
    },
    "apps": {
      "ios.release": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/BaseReactNative.app",
        "build": "xcodebuild -workspace ios/BaseReactNative.xcworkspace -scheme BaseReactNative -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 13 Pro Max'"
      },
      "ios.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/BaseReactNative.app",
        "build": "xcodebuild -workspace ios/BaseReactNative.xcworkspace -scheme BaseReactNative -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -destination 'name=iPhone 13 Pro Max'"
      }
    },
    "configurations": {
      "ios.sim.release": {
        "device": "simulator",
        "app": "ios.release"
      },
      "ios.sim.debug": {
        "device": "simulator",
        "app": "ios.debug"
      }
    }
  }
}
